{
  "name": "@safupad/sdk",
  "version": "3.0.3",
  "description": "TypeScript SDK for interacting with SafuPad smart contracts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "docs": "typedoc"
  },
  "keywords": [
    "safupad",
    "launchpad",
    "defi",
    "bnb",
    "bsc",
    "ethereum",
    "web3",
    "sdk",
    "typescript",
    "bonding-curve"
  ],
  "author": "SafuPad Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domistro16/safupadsdk.git"
  },
  "bugs": {
    "url": "https://github.com/domistro16/safupadsdk/issues"
  },
  "homepage": "https://github.com/domistro/safupadsdk#readme",
  "peerDependencies": {
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
